<Application x:Class="MIMP.WIMP.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MIMP.WIMP.WPF"
             Startup="Application_Startup"
             Exit="Application_Exit"
             x:Name="WIMP"
             StartupUri="/WIMP;component/mainwindow.xaml">
    <Application.Resources >
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/AppResourceDictionary.xaml"/>
                <ResourceDictionary Source="/Resources/EnumResourceDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
